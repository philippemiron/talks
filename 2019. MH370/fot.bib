@Article{Brazeau-etal-13,
  author        = {Brazeau, D. A., and M. P. Lesser, and M. Slattery},
  title         = {Genetic Structure in the Coral, Montastraea cavernosa: Assessing Genetic Differentiation among and within Mesophotic Reefs},
  volume        = {8},
  number        = {5},
  pages         = {1--11},
  abstract      = {Mesophotic coral reefs (30{\^a}$\neg${{\"{A}}}$\neg${\`{i}}150 m) have recently received increased
	attention as a potential source of larvae (e.g., the refugia hypothesis)
	to repopulate a select subset of the shallow water (30 m) coral
	fauna. To test the refugia hypothesis we used highly polymorphic
	Amplified Fragment Length Polymorphism (AFLP) markers as a means
	to assess small-scale genetic heterogeneity between geographic locations
	and across depth clines in the Caribbean coral, Montastraea cavernosa.
	Zooxanthellae-free DNA extracts of coral samples (N = 105) were analyzed
