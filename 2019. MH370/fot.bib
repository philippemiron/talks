@Article{Olascoaga-etal-06,
  author       = {Olascoaga, M. J., and Rypina, I. I., and Brown, M. G., and Beron-Vera, F. J., and Ko\c{c}ak, H., and Brand, L. E., and Halliwell, G. R., and Shay, L. K.},
  title        = {{Persistent transport barrier on the West Florida Shelf}},
  volume       = {33},
  pages        = {L22603},
  abstract     = {Analysis of drifter trajectories in the Gulf of Mexico has revealed
	the existence of a region on the southern portion of the West Florida
	Shelf (WFS) that is not visited by drifters that are released outside
	of the region. This so{\quotesinglbase}{{\"{A}}}{\^{e}}called ``forbidden zone'' (FZ) suggests
	the existence of a persistent cross{\quotesinglbase}{{\"{A}}}{\^{e}}shelf transport barrier on
	the southern portion of the WFS. In this letter a year{\quotesinglbase}{{\"{A}}}{\^{e}}long record
	of surface currents produced by a Hybrid{\quotesinglbase}{{\"{A}}}{\^{e}}Coordinate Ocean Model
	simulation of the WFS is used to compute Lagrangian coherent structures
	(LCSs), which reveal the presence of a persistent cross{\quotesinglbase}{{\"{A}}}{\^{e}}shelf transport
	barrier in approximately the same location as the boundary of the
	FZ. The location of the cross{\quotesinglbase}{{\"{A}}}{\^{e}}shelf transport barrier undergoes
	a seasonal oscillation, being closer to the coast in the summer than
	in the winter. A month{\quotesinglbase}{{\"{A}}}{\^{e}}long record of surface currents inferred
	from high{\quotesinglbase}{{\"{A}}}{\^{e}}frequency (HF) radar measurements in a roughly 60 km
	$\sqrt{}${\'{o}} 80 km region on the WFS off Tampa Bay is also used to compute
	LCSs, and these also reveal the presence of transient transport barriers.
	While the HF{\quotesinglbase}{{\"{A}}}{\^{e}}radar{\quotesinglbase}{{\"{A}}}{\^{e}}derived transport barriers cannot be unambiguously
	linked to the boundary of the FZ, this analysis does demonstrate
	the feasibility of monitoring transport barriers on the WFS using
	a HF{\quotesinglbase}{{\"{A}}}{\^{e}}radar{\quotesinglbase}{{\"{A}}}{\^{e}}based measurement system. The implications of a persistent
	cross{\quotesinglbase}{{\"{A}}}{\^{e}}shelf transport barrier on the WFS for the development of
	harmful algal blooms on the shoreward side of the barrier are considered.},
  bdsk-url-1   = {https://agupubs.onlinelibrary.wiley.com/doi/abs/10.1029/2006GL027800},
  bdsk-url-2   = {https://doi.org/10.1029/2006GL027800},
  year         = {2016},
  doi          = {10.1029/2006GL027800},
  eprint       = {https://agupubs.onlinelibrary.wiley.com/doi/pdf/10.1029/2006GL027800},
  journaltitle = {Geophysical Research Letters},
  keywords     = {forbidden zone, Lagrangian coherent structure, transport barrier},
}

