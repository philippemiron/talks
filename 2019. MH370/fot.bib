@misc{Shnirelman-99,
	Abstract = {{Summary: n the existing stability theory of steady flows of an ideal
	incompressible fluid, formulated by V. Arnold, stability is understood
	as stability with respect to perturbations with vorticity small in
	$L^2$. Nothing has been known about the stability under perturbations
	with small energy, without any restrictions on vorticity; it was
	clear that existing methods do not work for this (the most physically
	reasonable) class of perturbations. We prove that in fact every nontrivial
	steady flow is unstable in $L^2$; moreover, every flow may be transformed
	into any other one, with the same energy and momentum, with the help
	of an appropriately chosen perturbation with arbitrary small energy.
	This phenomenon reminds one of the Arnold diffusion. This result
	is proven by the direct construction of a growing perturbation, which
	is done by a variational method.}},
	Author = {Shnirelman, Alexander},
	year         = {1999},
	Howpublished = {{Proceedings of the conference on partial differential equations, Saint-Jean-de-Monts, France, May 31--June 4, 1999. Exp. Nos. I--XIX (1999). Nantes: Universit\'e de Nantes. Exp. No. XIII, 8 p. (1999).}},
	Language = {English},
	Title = {{On the $L^2$-instability and $L^2$-controllability of steady flows of an ideal incompressible fluid}}}

