@article{Hayes-Shepherd-89,
	Annote = {The classical problem of the response of a balanced, axisymmteric
	vortex to thermal and mechanical forcing is re-examined, paying special
	attention to the lower boundary condition. The correct condition
	is D@/Dt=0, where @ es the geopotential y D/Dt the material derivative,
	which explicity accounts for a mass redistribution as part of the
	mean-flow response. This redistribution is neglected when using the
	boundary Dp/Dt=0, which has conventionally been applied in this problem.
	It is shown that applying the incorrect boundary condition and thereby
	ignoring the surface pressure change, leads to a zonal winds aceleration
	du/dt that is too strong, specially near the surface. The effect
	is significant for planetary-scale forcing even when applied at tropopause
	level. A comparison is made between the mean-flow evolution in a
	baroclinic life-cycle, as simulated in a fully nonlinear, primitive
	equation model and that predicted by using the simulated eddy fluxes
	in the zonally-symmetric response problem. Use of the correct lower
	boundary condition is shown to lead to improved agreement.},
	Author = {Haynes, P. H. and T. G. Shepherd},
	Journal = {Quarterly Journal of the Royal Meteorological Society},
	Keywords = {vortice, atmosfera},
	Pages = {1181-1208},
	Title = {The importance of surface pressure changes in the response of the atmosphere to zonally-symmetric thermal and mechanical forcing},
	Volume = {115},
	Year = {1989}}

