@article{Vallis-86,
	Annote = {Most of the principal qualitative features of the El Niano-Southern
	Oscillation phenomenon can be explained by a simple but physically
	motivated theory. These features are the occurrence of sea-surface
	warmings in the eastern equatorial Pacific and the associated trade
	wind reversal; the aperidicity of these events; the preferred onset
	time with respect to the seasonal cycle; and thje much weaker events
	inthe Atlantic and Indian oceans. The theory, in its simplest form,
	is a conceptual model for the interaction of just three variables,
	namely near-surface temperatures in the east and west equatorial
	ocean and a wind driven current advecting the temperature field.
	For alrge range of parameters, the model is naturally chaotic and
	aperiodically produces El Niano-like events. For a smaller basin,
	representing a smaller ocean, the events are proportionally less
	intense.},
	Author = {Vallis, G.K.},
	Journal = {Science},
	Keywords = {ENSO, caos},
	Pages = {243-245},
